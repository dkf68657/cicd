{
  "family": "cicdservice",
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "arn:aws:iam::082651270708:role/MythicalMysfitsCoreStack-EcsServiceRole-11YCDP13WORX",
  "taskRoleArn": "arn:aws:iam::082651270708:role/MythicalMysfitsCoreStack-ECSTaskRole-1BK6RLPSRDAEF",
  "containerDefinitions": [
    {
      "name": "cicdservice",
      "image": "082651270708.dkr.ecr.us-east-1.amazonaws.com/cicd:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "http"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "cicd",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "awslogs-cicd-service"
        }
      },
      "essential": true
    }
  ]
}
