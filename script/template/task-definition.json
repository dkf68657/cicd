{
  "family": "%%FAMILY%%",
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "%%EcsServiceRole%%",
  "taskRoleArn": "%%ECSTaskRole%%",
  "containerDefinitions": [
    {
      "name": "%%ContainerName%%",
	  "image":brucedong1987/cicd:latest
      "portMappings": [
        {
          "containerPort": %%port%%,
          "protocol": "http"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "%%LOG_GROUP%%",
          "awslogs-region": "%%REGION%%",
          "awslogs-stream-prefix": "%%STREAM-PREFIX%%"
        }
      },
      "essential": true
    }
  ]
}
